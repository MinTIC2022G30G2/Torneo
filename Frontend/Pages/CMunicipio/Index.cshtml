@page
@model Frontend.Pages.CMunicipio.IndexModel
@{
    ViewData["Title"] = "Gestion Municipio";
}
<h2>@ViewData["Title"]</h2>
<hr/>
<p><a asp-page="./Create" class="btn btn-primary">Crear Municipio</a></p>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
            <th colspan="3"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.items){
            <tr>
                <td>@item.Id</td>
                <td>@item.Nombre</td>
                <td><a asp-page="./Edit" class="btn btn-success" asp-route-id="@item.Id"><i class="fa fa-home fa-fw"></i>Editar</a></td>
                <td><a asp-page="./Details" class="btn btn-info" asp-route-id="@item.Id">Detalle</a></td>
                <td><a asp-page="./Delete" class="btn btn-danger" asp-route-id="@item.Id">Eliminar</a></td>
            </tr>
        }
    </tbody>
</table>