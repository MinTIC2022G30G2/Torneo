@page
@model Frontend.Pages.CMunicipio.DeleteModel
@{
    ViewData["Title"]="Adicionar Municipio";
}
<h2>@ViewData["Title"]</h2>
@if(ViewData["Error"]==null){
    <h4 class="text-danger">Esta seguro de eliminar este municipio?</h4>
}else{
    <h4 class="text-danger">@ViewData["Error"]</h4>
}
<div class="form-horizontal">
	<hr/>
	<div class="row">
		<div class="col-md-4">
			<form method="post">
                <div class="form-group">
                    <label asp-for="Municipio.Id" class="control-label"></label>
                    <input asp-for="Municipio.Id" class="form-control" readonly/>
                </div>
                <div class="form-group">
                    <label asp-for="Municipio.Nombre" class="control-label"></label>
                    <input asp-for="Municipio.Nombre" class="form-control" readonly/>
                </div>
                <div class="form-group">
                    <input type="submit" value="Enviar" class="btn btn-primary"/>
                    <a asp-page="Index" class="btn btn-secondary">Cancelar</a>
                </div>
			</form>
		</div>
	</div>
</div>